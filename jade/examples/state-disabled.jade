        .form-group.col-md-12
          .checkbox-inline.switch
            input(type='checkbox', name='sw-bananas-3s' id='sw-bananas-3s', disabled)
            label(for='sw-bananas-3s') Bananas
          .checkbox-inline.switch
            input(type='checkbox', name='sw-apples-3s' id='sw-apples-3s', checked, disabled)
            label(for='sw-apples-3s') Apples
          .checkbox-inline.switch
            input(type='checkbox', name='oranges-3s' id='sw-oranges-3s', disabled)
            label(for='sw-oranges-3s') Oranges
      fieldset.row(disabled)
        .col-md-12
          legend
            span Switches based on checkboxes
        .form-group.col-md-6
          .checkbox.switch-with-text
            input(type='checkbox', name='switches-fruits', value='bananas', id='switch-bananas-1', checked)
            label(for='switch-bananas-1') Bananas
          .checkbox.switch-with-text
            input(type='checkbox', name='switches-fruits', value='apples', id='switch-apples-1')
            label(for='switch-apples-1') Apples
          .checkbox.switch-with-text
            input(type='checkbox', name='switches-fruits', value='oranges', id='switch-oranges-1')
            label(for='switch-oranges-1') Oranges
        .form-group.col-md-6
          .checkbox.switch
            input(type='checkbox', id='sw-bananas')
            label(for='sw-bananas') Bananas
          .checkbox.switch
            input(type='checkbox', id='sw-apples')
            label(for='sw-apples') Apples
          .checkbox.switch
            input(type='checkbox', id='sw-oranges', checked)
            label(for='sw-oranges') Oranges
      fieldset.row(disabled)
        .col-md-12
          legend
            span Switches inline based on checkboxes
        .form-group.col-md-12
          .checkbox-inline.switch-with-text
            input(type='checkbox', name='switches-fruits-2', value='bananas-2', id='switch-bananas-2')
