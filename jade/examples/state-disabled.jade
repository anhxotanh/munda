          .checkbox-inline.switch
            input(type='checkbox', id='sw-apples-3')
            label(for='sw-apples-3') Apples
          .checkbox-inline.switch
            input(type='checkbox', id='sw-oranges-3', checked)
            label(for='sw-oranges-3') Oranges
      footer.ff-rounded-b
        input(type='submit', name='submit', value='Submit', class='btn btn-primary ff-rounded', style='margin-right: 10px;')
