            input(type='checkbox', id='apples-3a', disabled)
            label(for='apples-3a') Apples
          .checkbox-inline
            input(type='checkbox', id='oranges-3a', disabled)
            label(for='oranges-3a') Oranges
      fieldset.row(disabled)
        .col-md-12
          legend
            span Inline radio buttons and checkboxes
        .form-group.col-md-12
