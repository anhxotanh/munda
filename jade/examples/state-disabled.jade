            input(type='radio', name='fruits-2', value='bananas-2', id='bananas-2', checked)
            label(for='bananas-2') Bananas
          .radio-inline
            input(type='radio', name='fruits-2', value='apples-2', id='apples-2')
            label(for='apples-2') Apples
          .radio-inline
            input(type='radio', name='fruits-2', value='oranges-2', id='oranges-2')
            label(for='oranges-2') Oranges
        .form-group.col-md-12
          .checkbox-inline
            input(type='checkbox', id='bananas-3', checked)
            label(for='bananas-3') Bananas
          .checkbox-inline
            input(type='checkbox', id='apples-3')
            label(for='apples-3') Apples
          .checkbox-inline
            input(type='checkbox', id='oranges-3')
            label(for='oranges-3') Oranges
      fieldset.row
        .col-md-12
          legend
            span Switches based on checkboxes
        .form-group.col-md-6
          .checkbox.switch-with-text
            input(type='checkbox', name='bananass', value='bananas', id='switch-bananas-1s', checked, disabled)
            label(for='switch-bananas-1s') Bananas
          .checkbox.switch-with-text
            input(type='checkbox', name='appless', value='apples', id='switch-apples-1s', disabled)
            label(for='switch-apples-1s') Apples
          .checkbox.switch-with-text
            input(type='checkbox', name='orangess', value='oranges', id='switch-oranges-1s', disabled)
            label(for='switch-oranges-1s') Oranges
        .form-group.col-md-6
          .checkbox.switch
            input(type='checkbox', name='sw-bananass' id='sw-bananass', disabled)
            label(for='sw-bananass') Bananas
          .checkbox.switch
            input(type='checkbox', name='sw-appless', id='sw-appless', checked, disabled)
            label(for='sw-appless') Apples
          .checkbox.switch
            input(type='checkbox', name='sw-orangess', id='sw-orangess', disabled)
            label(for='sw-orangess') Oranges
      fieldset.row
        .col-md-12
          legend
            span Switches inline based on checkboxes
        .form-group.col-md-12
          .checkbox-inline.switch-with-text
            input(type='checkbox', name='bananas-2s', value='bananas-2', id='switch-bananas-2s', checked, disabled)
            label(for='switch-bananas-2s') Bananas
          .checkbox-inline.switch-with-text
            input(type='checkbox', name='apples-2s', value='apples-2', id='switch-apples-2s', disabled)
            label(for='switch-apples-2s') Apples
          .checkbox-inline.switch-with-text
            input(type='checkbox', name='oranges-2s', value='oranges-2', id='switch-oranges-2s', disabled)
