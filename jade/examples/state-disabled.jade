      fieldset.row
        .col-md-12
          legend
            span Radio buttons and checkboxes
        .form-group.col-md-6
          .radio
            input(type='radio', name='fruitsb', value='bananas', id='bananas-1b', checked, disabled)
            label(for='bananas-1b') Bananas
          .radio
            input(type='radio', name='fruitsb', value='apples', id='apples-1b', disabled)
            label(for='apples-1b') Apples
          .radio
            input(type='radio', name='fruitsb', value='oranges', id='oranges-1b', disabled)
            label(for='oranges-1b') Oranges
        .form-group.col-md-6
          .checkbox
            input(type='checkbox', id='bananasb', checked, disabled)
            label(for='bananasb') Bananas
          .checkbox
            input(type='checkbox', id='applesb', disabled)
            label(for='applesb') Apples
          .checkbox
            input(type='checkbox', id='orangesb', disabled)
            label(for='orangesb') Oranges
      fieldset.row
        .col-md-12
          legend
            span Inline radio buttons and checkboxes
        .form-group.col-md-12
          .radio-inline
            input(type='radio', name='fruits-2a', value='bananas-2a', id='bananas-2a', checked, disabled)
            label(for='bananas-2a') Bananas
          .radio-inline
            input(type='radio', name='fruits-2a', value='apples-2a', id='apples-2a', disabled)
            label(for='apples-2a') Apples
